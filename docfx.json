{
  "metadata": [
    {
      "src": [
        {
          "src": "C:/Tecnocampus 5Ã©/TFG Videojocs/Unity_FSM_ToolKit",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "Assets/Plugins/**"
        ]
        },
        {
            "src": "C:/Program Files/Unity/Hub/Editor/2022.3.14f1/Editor/Data/Managed",
            "files": [
                "**/*.dll"
            ]
        }
      ],
      "dest": "api",
      "filter": "filter.yml",
      "properties": {
          "TargetFramework": "netstandard2.0",
          "DefineConstants": "UNITY_EDITOR"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "docs",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "",
      "_appTitle": "FSM Unity Framework",
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/favicon.ico",
      "_enableSearch": true,
      "pdf": false
    },
    "sitemap": {
      "baseUrl": "https://arnaucarbonell.github.io/fsm-unity-framework",
      "changefreq": "weekly",
      "fileOptions": {
          "api/*": {
              "changefreq": "daily"
            }
        }
    },
    "xref": [
        "https://arnaucarbonell.github.io/UnityXrefMaps/xrefmap.yml"
    ],
    "xrefService": [
        "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "dest": "../docs"
  }
}